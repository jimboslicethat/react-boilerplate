{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A React boilerplate using babel and webpack",
  "author": "James Walsh <jamesandersonwalsh@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./build && webpack",
    "start:dev": "npm run build && webpack-dev-server --content-base build/ --watch",
    "test": "mocha",
    "test:watch": "mocha -w",
    "lint": "eslint ./src",
    "precommit": "npm run lint && npm run test && npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "6.26.0",
    "chai": "3.5.0",
    "css-loader": "0.28.1",
    "eslint": "4.14.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-standard": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.3",
    "mocha": "3.4.1",
    "postcss": "6.0.1",
    "postcss-cssnext": "2.9.0",
    "postcss-import": "10.0.0",
    "postcss-loader": "2.0.5",
    "postcss-variables": "1.1.0",
    "react": "15.5.4",
    "react-css-modules": "4.3.0",
    "react-dom": "15.5.4",
    "sinon": "2.2.0",
    "sinon-chai": "2.10.0",
    "style-loader": "0.17.0",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5",
    "webpack-livereload-plugin": "0.11.0"
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-cssnext": {
        "compress": false
      },
      "postcss-variables": {}
    }
  }
}
