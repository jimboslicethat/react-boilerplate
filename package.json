{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A React boilerplate using babel and webpack",
  "author": "James Walsh",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./build && webpack",
    "start": "npm run build && webpack-dev-server --content-base build/ --watch",
    "test": "mocha",
    "test:watch": "mocha -w",
    "lint": "eslint ./src",
    "precommit": "npm run lint && npm run test && npm run build"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "css-loader": "0.28.1",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-jsx": "4.0.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-mocha": "4.9.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-standard": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.3",
    "mocha": "3.4.1",
    "postcss": "6.0.1",
    "postcss-cssnext": "2.9.0",
    "postcss-import": "10.0.0",
    "postcss-loader": "1.3.3",
    "postcss-variables": "1.1.0",
    "react": "15.5.4",
    "react-css-modules": "4.3.0",
    "react-dom": "15.5.4",
    "sinon": "1.17.6",
    "sinon-chai": "2.10.0",
    "style-loader": "0.17.0",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-livereload-plugin": "0.11.0"
  },
  "dependencies": {}
}
